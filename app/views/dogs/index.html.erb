<h1>Dogs</h1>

<div id="search-form">
  <%= render 'search_form' %>
</div>

<table>
  <thead>
    <tr id="dogs-header">
      <%= render 'dogs_header' %>
    </tr>
  </thead>
  <tbody id="dogs">
    <%= render @dogs %>
  </tbody>
</table>
<div id="paginator">
  <%= paginate @dogs, remote: true %>
</div>
